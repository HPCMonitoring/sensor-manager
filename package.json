    "style": "npx tailwindcss -i src/index.css -o src/index.css"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "typescript": "^4.9.3",
    "autoprefixer": "^10.4.13",
    "barrelsby": "^2.5.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.0"
