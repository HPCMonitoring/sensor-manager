{
  "name": "sensor-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "barrels": "barrelsby --config .barrelsby.json",
    "lint": "eslint '**/*.{tsx,ts,js}'",
